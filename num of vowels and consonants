sentence = input("Enter a sentence: ")
vowels = "aeiouAEIOU"
num_vowels = 0
num_consonants = 0
for char in sentence:
    if char.isalpha():  
        if char in vowels:
            num_vowels += 1
        else:
            num_consonants += 1
print("Number of vowels =",num_vowels)
print("Number of consonants =",num_consonants)
if num_vowels > num_consonants:
    print("Vowels are more.")
elif num_consonants > num_vowels:
    print("Consonants are more.")
else:
    print("The number of vowels and consonants are equal.")
